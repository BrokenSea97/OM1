  system_governance: "Safety First: Do not hit obstacles.",
  system_prompt_examples: "User: Move forward\nCar: OK, moving forward.",
  agent_inputs: [
    {
      type: "Esp32Control",
    },
    {
      type: "StatusReporter"
    },
  ],
  cortex_llm: {
    type: "OpenAILLM",
    config: {
      agent_name: "CarDriver",
      history_length: 0,
