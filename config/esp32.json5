//      model: "deepseek-chat"
    },
  },
  agent_actions: [
    {
      name: "speak",
      llm_label: "speak",
      implementation: "passthrough",
      connector: "console"
    },
    {
      name: "motor",
      llm_label: "motor",
      implementation: "actions.motor.connector.serial_control.SerialMotor",
      config: {
        port: "COM3",
        baudrate: 115200
      },
      connector: "serial_control"
    },
